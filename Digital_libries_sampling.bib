author = {Deodhar, Swanand J. and Saxena, K. B. C. and Ruohonen, Mikko},
title = {Firm-oriented success factors of an open source software (OSS) product},
year = {2010},
isbn = {9781605589787},
publisher = {Association for Computing Machinery},
