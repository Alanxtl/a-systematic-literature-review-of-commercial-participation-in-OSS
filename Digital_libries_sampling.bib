@article{10.1145/3277539.3286730,
author = {Neville-Neil, George},
title = {A Chance Gardener: Harvesting open-source products and planting the next crop},
year = {2018},
issue_date = {July-August 2018},
publisher = {Association for Computing Machinery},
address = {New York, NY, USA},
